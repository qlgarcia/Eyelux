{
  "name": "eyelux-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "vercel-build": "prisma generate && DATABASE_URL=\"file:./dev.db\" prisma db push && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "node prisma/seed.js",
    "db:seed:admin": "tsx prisma/seed-admin.ts",
    "db:seed:products": "tsx prisma/seed-products.ts",
    "db:reset": "rm -f prisma/dev.db && prisma db push && npm run db:seed:admin",
    "postinstall": "prisma generate"
  },
